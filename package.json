{
  "name": "rnquickstart",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "local": "./update-node_modules.sh && ENVFILE=.env react-native run-android",
    "dev": "./update-node_modules.sh && ENVFILE=.env.development react-native run-android",
    "prod": "./update-node_modules.sh && ENVFILE=.env.production react-native run-android",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "cd ios && pod install",
    "start": "react-native start",
    "build:local": "./update-node_modules.sh && cd android && ENVFILE=.env ./gradlew assembleRelease",
    "build:dev": "./update-node_modules.sh && cd android && ENVFILE=.env.development ./gradlew assembleRelease",
    "build:prod": "./update-node_modules.sh && cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "install-apk": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "node scripts/react-scrollview-fix.js"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/netinfo": "^5.7.0",
    "i18n-js": "^3.5.1",
    "lodash": "^4.17.15",
    "ramda": "^0.27.0",
    "react": "16.11.0",
    "react-native": "0.62.1",
    "react-native-config": "^1.0.0",
    "react-native-device-info": "^5.5.4",
    "react-native-exit-app": "^1.1.0",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-largelist-v3": "^3.0.15",
    "react-native-localize": "1.3.1",
    "react-native-pose": "^0.9.1",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^2.4.0",
    "react-native-sound": "^0.11.0",
    "react-native-spring-scrollview": "^2.0.23",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-view": "^2.14.2",
    "react-native-webview": "^9.4.0",
    "react-navigation": "3.13.0",
    "react-navigation-hooks": "^1.1.0",
    "react-navigation-redux-helpers": "3.0.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-config": "^0.1.1",
    "redux-persist": "5.6.12",
    "redux-saga": "^1.1.3",
    "teaset": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-eslint": "8.2.4",
    "babel-jest": "^24.9.0",
    "babel-plugin-root-import": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "1.13.5",
    "react-test-renderer": "16.11.0",
    "redux-logger": "2.8.2"
  },
  "jest": {
    "preset": "react-native"
  }
